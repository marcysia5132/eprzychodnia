<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PacOdwWizytyPotw">

    <Button
        android:id="@+id/PacOdwWizytyPotw_PrzyciskTak"
        android:layout_width="127dp"
        android:layout_height="56dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="52dp"
        android:text="TAK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/PacOdwWizytyPotw_PrzyciskNie"
        android:layout_width="127dp"
        android:layout_height="56dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="156dp"
        android:text="NIE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/PacOdwWizytyPotw_tekst"
        android:layout_width="335dp"
        android:layout_height="27dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="92dp"
        android:gravity="center"
        android:text="Czy napewno chcesz odwołać wizytę?"
        android:textColor="#1F36B1"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/datateks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="278dp"
        android:text="Twoja Data:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>