<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PacPotwWizyty">

    <TextView
        android:id="@+id/PacPotwWizyty_szczegoly"
        android:layout_width="387dp"
        android:layout_height="63dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="184dp"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/PacPotwWizte_przycisk"
        android:layout_width="303dp"
        android:layout_height="53dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="384dp"
        android:text="Potwierdzam"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="379dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="102dp"
        android:text="Chcesz się zapisać na następującą wizytę:"
        android:textColor="#081C76"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/PacPotwWizyty_lekarz"
        android:layout_width="383dp"
        android:layout_height="81dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="276dp"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>